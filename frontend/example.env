# =============================================================================
# Frontend Environment Configuration
# =============================================================================

# API Configuration
VITE_API_BASE_URL=/api/v1

# Development Settings
VITE_DEV_MODE=true
VITE_MOCK_API=false

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_LOGGING=true

# UI Configuration
VITE_THEME=light
VITE_LANGUAGE=en
VITE_TIMEZONE=UTC

# Performance Settings
VITE_CACHE_TTL=300
VITE_REQUEST_TIMEOUT=10000
VITE_MAX_RETRIES=3

# Security Settings
VITE_ENABLE_AUTH=true
VITE_SESSION_TIMEOUT=3600
VITE_REFRESH_TOKEN_INTERVAL=300

# Monitoring & Analytics
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ANALYTICS_ID=

# Export Configuration
VITE_EXPORT_MAX_SIZE=1000000
VITE_EXPORT_TIMEOUT=300000

# Model Management
VITE_MODEL_VALIDATION_TIMEOUT=60000
VITE_MODEL_DEPLOYMENT_TIMEOUT=120000 